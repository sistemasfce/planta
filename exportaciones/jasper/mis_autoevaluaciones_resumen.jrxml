	<parameter name="persona" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="ciclo" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT 		asignaciones.asignacion,
                	personas.documento,
                	personas.apellido,
                	personas.nombres,
		personas.apellido || ', ' || personas.nombres as nombre_completo,
                	actividades.descripcion as actividad_desc,
                	ubicaciones.codigo as ubicacion_desc,
		dimensiones.codigo as dimension_desc,
		autoevaluaciones_por_act.calificacion,
		(SELECT ceiling	(
		(SELECT SUM(puntaje)
FROM autoevaluaciones_por_act LEFT OUTER JOIN evaluaciones_calificaciones ON autoevaluaciones_por_act.calificacion = evaluaciones_calificaciones.descripcion
WHERE autoevaluaciones_por_act.persona = $P{persona}
			AND autoevaluaciones_por_act.ciclo_lectivo = $P{ciclo}
			AND autoevaluaciones_por_act.estado = 1
			AND autoevaluaciones_por_act.calificacion <> 'No se realizo')::float /
	(SELECT COUNT (*) as cantidad
FROM autoevaluaciones_por_act
WHERE autoevaluaciones_por_act.persona = $P{persona}
			AND autoevaluaciones_por_act.ciclo_lectivo = $P{ciclo}
			AND autoevaluaciones_por_act.estado = 1
			AND calificacion <> 'No se realizo')::float
			))::float as total


            FROM asignaciones LEFT OUTER JOIN personas ON (asignaciones.persona = personas.persona)
            LEFT OUTER JOIN autoevaluaciones_por_act ON (asignaciones.asignacion = autoevaluaciones_por_act.asignacion)
            LEFT OUTER JOIN actividades ON (asignaciones.actividad = actividades.actividad)
            LEFT OUTER JOIN ubicaciones ON (asignaciones.ubicacion = ubicaciones.ubicacion)
LEFT OUTER JOIN dimensiones ON (asignaciones.dimension = dimensiones.dimension)

             WHERE         personas.persona = $P{persona}
			AND autoevaluaciones_por_act.ciclo_lectivo = $P{ciclo}
			AND autoevaluaciones_por_act.estado = 1

            ORDER BY dimension_desc]]>
	</queryString>
